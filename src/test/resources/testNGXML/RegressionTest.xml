<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Suite" parallel="methods" data-provider-thread-count="1">
    <listeners>
        <listener class-name="com.Test.Arunkumar.Framework.TestNGListener"></listener>
    </listeners>
    <test verbose="1" preserve-order="true"
          name="Regression Suite">
        <classes>
            <class name="com.Test.Arunkumar.TestNGRunner.RegressionTestngRunner" />
            <parameter name="executionMode" value="LOCAL"></parameter>
            <parameter name="browser" value="CHROME"></parameter>
        </classes>
    </test>
</suite>